  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2020",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "outDir": "lib"
  },
  "exclude": ["lib", "node_modules"],
  "include": ["src/**/*"]
}
